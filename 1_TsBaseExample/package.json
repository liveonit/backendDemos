{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "clean": "rm -rf build/",
    "copy-files": "copyfiles -u 1 src/**/*.sql build/",
    "build": "npm run clean && tsc && tsc-alias && npm run copy-files",
    "tsc": "tsc",
    "start": "tsc && node .",
    "dev": "nodemon -w src --ext ts,sql --exec ts-node -r tsconfig-paths/register src/index.ts",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "prettier:format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.0.3"
  }
}
