{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "clean": "rm -rf build/",
    "copy-files": "copyfiles -u 1 src/**/*.sql build/",
    "build": "npm run clean && tsc && tsc-alias && npm run copy-files",
    "development": "nodemon -w src --ext ts,sql --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node build/index.js",
    "typeorm:migration:create": "npx ts-node src/utils/dbMigrations/createMigration.ts migration -- ",
    "typeorm:seed:create": "npx ts-node src/utils/dbMigrations/createMigration.ts seed -- ",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "prettier:check": "prettier --check --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "prettier:format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.9.0",
    "argon2": "^0.30.2",
    "axios": "^1.1.3",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fuse.js": "^6.6.2",
    "graphql": "^15.3.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.9.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lazy": "^1.0.11",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.6",
    "ws": "^8.8.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/register": "^7.17.7",
    "@faker-js/faker": "^7.6.0",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.11.9",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "graphql-request": "^4.2.0",
    "jest": "^29.3.0",
    "jest-cli": "^29.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.6.11",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  }
}
